<li class="d-flex">
  <a class="text-decoration-none link-dark" href="/profile/{{this.id}}"><strong
    >{{this.firstName}} {{this.lastName}}</strong></a>
  <span class="d-flex astral-description">
    <div class="d-flex">
      <i class="bi bi-sun"></i>
      <p>{{this.sunSign.name}}</p>
    </div>
    <div class="d-flex">
      <i class="bi bi-moon"></i>
      <p>{{this.moonSign.name}}</p>
    </div>
    <div class="d-flex">
      <i class="bi bi-arrow-up"></i>
      <p>{{this.ascendantSign.name}}</p>
    </div>
  </span>
</li>